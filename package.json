{
   "name": "sis_ary_backend",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "author": {
      "name": "Andr√© Camargo"
   },
   "scripts": {
      "build": "tsc",
      "start": "node build/server.js",
      "dev": "ts-node-dev src/server.ts",
      "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
      "migration:show": "npm run typeorm -- -d ./src/database/data-source.ts migration:show",
      "migration:create": "ts-node ./src/database/createMigration.ts",
      "migration:registry": "npm run typeorm -- -d ./src/database/data-source.ts migration:run",
      "migration:revert": "npm run typeorm -- -d ./src/database/data-source.ts migration:revert",
      "migrasave": "ts-node-dev ./node_modules/typeorm/cli.js -d ./src/database/data-source.ts migration:run",
      "ts-prune": "ts-prune"
   },
   "dependencies": {
      "bcrypt": "^5.1.1",
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "express-async-errors": "^3.1.1",
      "jsonwebtoken": "^9.0.2",
      "nodemailer": "^6.9.16",
      "pg": "^8.4.0",
      "puppeteer": "^23.10.4",
      "react": "^19.0.0",
      "react-dom": "^19.0.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.17",
      "uuid": "^9.0.0"
   },
   "devDependencies": {
      "@types/bcrypt": "^5.0.0",
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.17",
      "@types/jsonwebtoken": "^9.0.3",
      "@types/node": "^16.11.10",
      "@types/nodemailer": "^6.4.17",
      "@types/react": "^19.0.1",
      "@types/react-dom": "^19.0.2",
      "@types/uuid": "^9.0.3",
      "ts-node": "^10.7.0",
      "ts-node-dev": "^2.0.0",
      "ts-prune": "^0.10.3",
      "typescript": "5.2.2"
   }
}
